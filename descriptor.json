{
    "name": "IA results uploader",
    "description": "This job has the responsability of upload the results generated by the IA algorithms",
    "container-image": {
      "image": "cellsia/s_ia_results_uploader",
      "type": "singularity"
    },
    "command-line": "[CYTOMINE_HOST] [CYTOMINE_PUBLIC_KEY] [CYTOMINE_PRIVATE_KEY]",
    "inputs": [
      {
        "id": "cytomine_host",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine host",
        "set-by-server": true,
        "optional": false,
        "type": "String"
      },
      {
        "id": "cytomine_public_key",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine public key",
        "set-by-server": true,
        "optional": false,
        "type": "String"
      },
      {
        "id": "cytomine_private_key",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine private key",
        "set-by-server": true,
        "optional": false,
        "type": "String"
      },
      {
        "id": "cytomine_id_project",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine project ID",
        "set-by-server": true,
        "optional": false,
        "type": "Number"
      },
      {
        "id": "cytomine_id_software",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine software ID",
        "set-by-server": true,
        "optional": false,
        "type": "Number"
      },
      {
        "id": "cytomine_image",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Cytomine image instance",
        "set-by-server": false,
        "optional": false,
        "type": "Domain",
        "uri": "/api/project/$currentProject$/imageinstance.json",
	    "uri-print-attribute": "instanceFilename",
	    "uri-sort-attribute": "instanceFilename"
      },
      {
	    "id": "cytomine_id_term",
	    "value-key": "[@ID]",
	    "command-line-flag": "--@id",
	    "name": "Term to predict",
	    "description": "The term to which generated detections will be associated",
	    "optional": true,
	    "type": "Domain",
	    "uri": "/api/ontology/$currentOntology$/term.json",
	    "uri-print-attribute": "name",
	    "uri-sort-attribute": "name"
      },
      {
        "id": "annotations",
        "value-key": "[@ID]",
        "command-line-flag": "--@id",
        "name": "Json string containing annotations generated by the IA",
        "set-by-server": false,
        "optional": false,
        "type": "String"
      }
    ],
  
    "schema-version": "cytomine-0.1"
  }
  